{
  "name": "swaggerfyts",
  "version": "0.0.0",
  "description": "SwaggerfyTs is an unopinonated tool which uses static code analysis to create a OpenAPI  specifications from typescript code, compatible with most web frameworks.",
  "author": "MartinRamm",
  "license": "Unlicense",
  "homepage": "https://github.com/swaggerfyts/swaggerfyts",
  "repository": {
    "type": "git",
    "url": "https://github.com/swaggerfyts/swaggerfyts"
  },
  "main": "lib/swaggerfyts.ts",
  "scripts": {
    "lint": "../../node_modules/.bin/eslint .",
    "build": "../../node_modules/.bin/tsc --project tsconfig.json",
    "test": "pnpm run build && ./node_modules/.bin/ava"
  },
  "keywords": [
    "typescript",
    "swagger",
    "openAPI",
    "static code analysis",
    "generate spec",
    "generate specification",
    "generate openapi",
    "generate swagger",
    "from code",
    "code to spec"
  ],
  "dependencies": {
    "effect": "2.0.0-next.31",
    "ts-morph": "^19.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "ava": "^5.3.1",
    "expect-type": "^0.16.0"
  }
}
